if test "${enable_pdfdoc+set}" = set; then :
  enableval=$enable_pdfdoc; case "${enableval}" in
             (yes) pdfdoc=true ;;
             (no)  pdfdoc=false ;;
             (*)   as_fn_error $? "wrong argument to --enable-pdfdoc" "$LINENO" 5 ;;
  esac
else
  case "no" in
             (yes) pdfdoc=true ;;
             (no)  pdfdoc=false ;;
  esac

fi



