LOAD FILE=../../../../plumedOpenACC.so
c1: CONTACT_MATRIX ...
  GROUPA=1-3
  GROUPB=1-108
  SWITCH={RATIONAL R_0=1 NN=6 MM=12 D_MAX=2}
  USEGPU
...

PRINT ARG=c1 FILE=colvar FMT=%8.4f

ones: ONES SIZE=108
cc: MATRIX_VECTOR_PRODUCT ARG=c1,ones 
PRINT ARG=cc FILE=coords FMT=%8.5f

mtc: MORE_THAN ARG=cc SWITCH={RATIONAL D_0=2 R_0=2}
s: SUM ARG=mtc PERIODIC=NO
PRINT ARG=s FILE=fcolvar FMT=%8.5f
BIASVALUE ARG=s
