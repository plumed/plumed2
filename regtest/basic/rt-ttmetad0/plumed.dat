 TEMP=300.0
 PACE=500
 FILE=HILLS-nonperiodic-case1
 GRID_RFILE=grid-nonperiodic-case1
 GRID_MIN=0
 GRID_MAX=pi
 GRID_BIN=10
... METAD

METAD ...
 LABEL=metadnp2
 ARG=psi
 SIGMA=0.20
 HEIGHT=1.20
 CALC_TRANSITION_BIAS
 TRANSITIONWELL0=1.0
 TRANSITIONWELL1=2.0
 TEMP=300.0
 PACE=500
 FILE=HILLS-nonperiodic-case2
 GRID_RFILE=grid-nonperiodic-case2
 GRID_MIN=0
 GRID_MAX=pi
 GRID_BIN=10
... METAD

METAD ...
