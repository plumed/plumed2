d1: DISTANCE ATOMS=1,2 
d2: DISTANCE ATOMS=3,4

c1: COLLECT_FRAMES ATOMS=1-20 ARG1=d1 ARG2=d2 STRIDE=1
ww: CUSTOM ARG1=c1.logweights FUNC=exp(x) PERIODIC=NO
r1: DISSIMILARITIES GROUP1=c1.d1 GROUP2=c1.d2 
PRINT ARG=r1 FILE=mymatrix.matx FMT=%8.4f
l1: LANDMARK_SELECT_STRIDE ARG1=r1 DATA=c1 NLANDMARKS=5
voro: VORONOI ARG=l1.r1 
lwe: MATRIX_VECTOR_PRODUCT WEIGHT=voro VECTOR=ww 
PRINT ARG=l1.*,lwe FILE=configs-stride.pdb FMT=%8.4f
