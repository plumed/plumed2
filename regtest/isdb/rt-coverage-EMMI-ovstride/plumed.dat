SIGMA_MIN=0.01 RESOLUTION=0.1 NOISETYPE=GAUSS
WRITE_STRIDE=1000 SIGMA0=0.2 DSIGMA=0.0

WRITE_OV_STRIDE=2 WRITE_OV=ovfile.dat
...


# printout


PRINT ARG=*.scoreb FILE=COLVAR STRIDE=1 
